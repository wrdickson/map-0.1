<div id="workspaceTop">
    <span style="font-size: 16px"><b>My Maps</b></span>
    <button type="button" id="closeWorkspace" class="pull-right btn btn-default btn-xs"><span class="fa fa-window-close"></span></button>
    <button type="button" id="maxWorkspace" class="pull-right btn btn-default btn-xs"><span class= "fa fa-window-restore"></span></button>
    <button type="buton" id="minWorkspace" class="pull-right btn btn-default btn-xs"><span class="fa fa-window-minimize"></span></button>
</div><br/>
<div id="workspaceControl">

    <div id="workspaceToolbar" class="btn-group" role="group" aria-label="...">
        <button title="Add map" type="button" class="btn btn-default btn-sm" id="btnCreateMap"><span class="fa fa-map"></span><span class="fa fa-plus" style="color: green"></span></button>
        <button title="Add directory" type="button" class="btn btn-default btn-sm" id="btnCreateMapDirectory"><span class="fa fa-folder-o"></span><span class="fa fa-plus" style="color: green"></span></button>
        <button title="Rename" type="button" class="btn btn-default btn-sm" id="btnRenameMap"><span class="fa fa-pencil"></span></button>
        <button title="Delete" type="button" class="btn btn-default btn-sm" id="btnDeleteMap"><span class="fa fa-times" style="color: red"></span></button>
    </div>

    <div id="workspaceContent">
    </div>
</div>
